{
  "name": "kevinquinnyo/code-challenges",
  "description": "Some Code Challenges",
  "type": "code-samples",
  "homepage": "https://github.com/kevinquinnyo/code-challenges",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/kevinquinnyo/code-challenges/issues"
  },
  "require": {
    "php": ">= 7.1",
    "cakephp/collection": "^3.6",
    "nesbot/carbon": "^2.7",
    "squizlabs/php_codesniffer": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "kevinquinnyo\\CC\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "kevinquinnyo\\CC\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always --configuration phpunit.xml.dist"
  },
  "config": {
    "sort-packages": true
  }
}
